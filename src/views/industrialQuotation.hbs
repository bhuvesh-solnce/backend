        :root {
            --primary-color: #0067CA;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            font-family: "DM Sans", serif;
        }

        @page {
            margin: 0;
            size: auto 100%;
        }

        @media print {
            .page-break {
                page-break-before: always;
            }

            .btn {
                background-color: var(--primary-color);
            }
        }

        img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }

        a {
            text-decoration: none;
            color: black;
            display: inline-block;
        }

        .txt-center {
            text-align: center;
        }

        .page-one {
            position: relative;
            z-index: 1;
            height: 100vh;
        }

        .DMsans-300 {
            font-family: "DM Sans", serif;
            font-weight: 300;
        }

        .DMsans-400 {
            font-family: "DM Sans", serif;
            font-weight: 400;
        }

        .DMsans-500 {
            font-family: "DM Sans", serif;
            font-weight: 500;
        }

        .DMsans-600 {
            font-family: "DM Sans", serif;
            font-weight: 600;
        }

        .DMsans-700 {
            font-family: "DM Sans", serif;
            font-weight: 700;
        }

        .DMsans-800 {
            font-family: "DM Sans", serif;
            font-weight: 800;
        }

        .poppins-400 {
            font-family: "Poppins", serif;
            font-weight: 400;
        }

        .poppins-600 {
            font-family: "Poppins", serif;
            font-weight: 600;
        }

        .theme-color {
            color: var(--primary-color);
        }

        .page-one-before {
            top: 0;
            left: 0;
            width: 100%;
            z-index: -5;
            align-items: center;
            justify-content: center;
            position: absolute;
        }

        .social-wrapper {
            display: flex;
            align-items: center;
            justify-content: center;
            color: var(--primary-color);
            font-size: 13px;
            gap: 6px;
            padding-top: 42px;
            padding-bottom: 50px;
        }

        .social-icon-wrapper {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 3px;
        }

        .social-icon-wrapper a {
            width: 24px;
            height: 24px;
        }

        .pdf-logo {
            margin: auto;
            width: 44vw;
            margin-top: 1rem;
            z-index: -1;
        }

        .banner-title {
            font-size: 40px;
            text-align: center;
            margin-top: 36px;
            letter-spacing: 2px;
        }

        .one-stop {
            font-size: 44px;
            color: var(--primary-color);
        }

        .download-txt {
            text-align: center;
            font-size: 25px;
            margin-top: 40px;
            margin-bottom: 6px;
        }

        .download-wrappper {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 4px;
        }

        .download-wrappper a {
            width: 175px;
        }

        .page-one-bottom {
            width: 100%;
            align-items: center;
            bottom: 0;
            margin-top: 45px;
        }

        .page-one-bottom img {
            width: 100%;
            height: auto;
            max-height: 45vh;
            display: block;
            object-fit: fill;
            vertical-align: bottom;
        }

        .copy-right {
            position: absolute;
            bottom: 2rem;
            color: white;
            width: 100%;
        }

        /* page two */

        .page-two {
            position: relative;
        }

        .page-two-top {
            padding: 30px;
        }

        .site-logo {
            width: 160px;
        }

        .banner-top {
            display: flex;
            justify-content: space-between;
            align-items: start;
        }

        .banner-top .social-wrapper {
            padding: 0;
        }

        .fs-28 {
            font-size: 28px;
            line-height: 1.5;
        }

        .fs-27 {
            font-size: 27px;
            line-height: 1.5;
        }

        .fs-26 {
            font-size: 26px;
            line-height: 1.5;
        }

        .fs-25 {
            font-size: 25px;
            line-height: 1.5;
        }

        .fs-24 {
            font-size: 24px;
            line-height: 1.5;
        }

        .fs-23 {
            font-size: 23px;
            line-height: 1.5;
        }

        .fs-22 {
            font-size: 22px;
            line-height: 1.5;
        }

        .fs-21 {
            font-size: 21px;
            line-height: 1.5;
        }

        .fs-20 {
            font-size: 20px;
            line-height: 1.5;
        }

        .fs-19 {
            font-size: 19px;
            line-height: 1.5;
        }

        .fs-18 {
            font-size: 18px;
            line-height: 1.5;
        }

        .fs-16 {
            font-size: 16px;
            line-height: 1.5;
        }

        .fs-15 {
            font-size: 15px;
            line-height: 1.5;
        }

        .page-two-list {
            list-style: none;
        }

        .page-two-list li {
            position: relative;
            padding-left: 24px;
        }

        .page-two-list li::before {
            content: '';
            background-image: url(./images/arrow.svg);
            width: 16px;
            height: 8px;
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            left: 0;
        }

        .footer {
            font-size: 16px;
            line-height: 1;
            text-align: center;
            position: absolute;
            top: 1095px;
            width: 100%;
            z-index: 1;
        }

        .footer a {
            background-color: white;
            padding: 0 24px;
        }

        .footer::before {
            content: '';
            position: absolute;
            top: 50%;
            width: 100%;
            transform: translateY(-50%);
            left: 0;
            border: 0.5px solid var(--primary-color);
            z-index: -1;
        }

        /* page three */

        .page-three {
            position: relative;
        }

        .page-three-top {
            padding: 30px;
        }

        .page-three-desc p {
            margin-bottom: 10px;
        }

        .btn {
            padding: 6px 30px;
            background-color: var(--primary-color);
            color: white;
            border-radius: 10px;
            display: inline-block;
            text-align: center;
            margin: auto;
        }

        .bank-grid {
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
            align-items: center;
            width: 660px;
            margin: 0 auto;
        }

        .bank-grid-col img {
            object-fit: contain;
        }

        .bank-grid-col {
            width: 25%;
            padding: 16px;
            height: 64px;
            border: none;
            border-right: 0.5px solid #EBECEC;
            border-bottom: 0.5px solid #EBECEC;
        }

        .bank-grid-col:nth-child(4n) {
            border-right: none;
        }

        .solar-grid {
            display: flex;
            flex-wrap: wrap;
            align-items: center;
            justify-content: center;
        }

        .solar-grid-col {
            width: 16.66%;
            padding: 12px;
            height: 80px;
            border: none;
            border-right: 0.5px solid #EBECEC;
            border-bottom: 0.5px solid #EBECEC;
        }

        .solar-grid-col:nth-child(6n) {
            border-right: none;
        }

        .solar-grid-col:nth-last-child(-n+6) {
            border-bottom: none;
        }

        .solar-grid-col img {
            object-fit: contain;
            max-width: 100%;
            height: 100%;
        }

        /* page four */

        .page-four {
            position: relative;
        }

        .page-four-top {
            padding: 30px;
        }

        .page-four-logo {
            width: 100%;
            margin: 0 auto;
            margin-top: 1.3rem;
            margin-bottom: -1rem;
            max-width: 180px;
        }

        .page-four-table {
            width: 100%;
            border-collapse: collapse;
        }

        .page-four-table th,
        .page-four-table td {
            border: 1px solid var(--primary-color);
            font-size: 15px;
            font-family: "DM Sans", serif;
            font-weight: 600;
            padding: 0 16px;
        }

        .page-four-table th {
            font-size: 16px;
            font-weight: 600;
            background-color: var(--primary-color);
            color: white;
            text-align: left;
            height: 48px;
        }

        .page-four-table tr {
            height: 64px;
            background-color: #F5F5F5;
        }

        .page-four-table tr:nth-child(even) {
            background-color: white;
        }

        .page-four-table tr td:last-child {
            width: 165px;
        }

        .page-five-table {
            width: 100%;
            border-collapse: collapse;
        }

        .page-five-table th,
        .page-five-table td {
            border: 1px solid var(--primary-color);
            font-size: 15px;
            font-family: "DM Sans", serif;
            font-weight: 600;
            padding: 0 16px;
        }

        .page-five-table th {
            font-size: 16px;
            font-weight: 600;
            background-color: var(--primary-color);
            color: white;
            text-align: left;
            height: 48px;
        }

        .page-five-table tr {
            height: 48px;
            background-color: #F5F5F5;
        }

        .page-five-table tr:nth-child(even) {
            background-color: white;
        }

        .page-five-table tr td:last-child {
            width: 165px;
        }

        .view-grid {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr;
            gap: 16px;
        }

        .view-card {
            width: 100%;
            height: 250px;
            background-color: var(--primary-color);
            border-radius: 16px;
            margin-bottom: 6px;
        }

        /* page five */

        .page-five {
            position: relative;
        }

        .page-five-padding {
            padding: 30px;
        }

        .note {
            padding: 4px 20px 4px 30px;
            background-color: var(--primary-color);
            display: inline-block;
            color: white;
            font-family: 16px;
        }

        .note-list li {
            color: black;
            list-style: none;
            padding-left: 20px;
            position: relative;
            line-height: 1.2;
        }

        .note-list li::before {
            content: '';
            width: 6px;
            height: 6px;
            border-radius: 100%;
            background-color: var(--primary-color);
            position: absolute;
            top: 8px;
            left: 0;
        }

        /* page six */

        .page-six {
            position: relative;
        }

        .page-six-padding {
            padding: 30px;
        }

        .scope {
            font-size: 32px;
            line-height: 1.5;
            text-align: center;
            color: var(--primary-color);
        }

        /* page seven */

        .page-seven {
            position: relative;
        }

        .page-seven-padding {
            padding: 30px;
        }

        .saving-grid {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr;
            gap: 16px;
        }

        .saving-card-box {
            padding: 16px;
            border-radius: 10px;
            background-color: var(--primary-color);
            color: white;
        }

        .sav-border {
            width: 60px;
            border: 1px solid #FFED00;
            margin: 10px auto;
        }

        /* page eight */

        .page-eight {
            position: relative;
        }

        .page-eight-padding {
            padding: 30px;
        }

        .weather-grid {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr;
            gap: 12px;
        }

        .weather-grid-col {
            border-radius: 16px;
            overflow: hidden;
        }

        .predict-list li::before {
            top: 10px;
        }

        /* page nine */
        .page-nine {
            position: relative;
        }

        .page-nine-padding {
            padding: 30px;
        }

        .scope-card {
            display: flex;
            align-items: center;
            gap: 16px;
            margin-bottom: 24px;
        }

        .scope-card-img {
            width: 100%;
            max-width: 340px;
            height: 200px;
            border-radius: 24px;
            overflow: hidden;
        }

        .scope-list {
            display: flex;
            flex-direction: column;
            gap: 4px;
        }

        /* page eleven */

        .page-eleven {
            position: relative;
        }

        .page-ele-padding {
            padding: 30px;
        }

        /* page thirteen */

        .page-thirteen {
            position: relative;
            min-height: 100vh;
            z-index: 1;
        }

        .page-13-before {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: calc(100% - 7rem);
            z-index: -1;
        }

        .page-13-logo {
            max-width: 256px;
            margin: auto;
            padding-top: 72px;
            margin-bottom: 3.5rem;
        }

        .thank-txt {
            font-size: 74px;
            color: white;
            letter-spacing: -0.08px;
            line-height: 1;
            margin-bottom: 1.5rem;
        }

        .app-txt {
            color: white;
        }

        .app-white {
            display: flex;
            align-items: center;
            justify-content: center;
        }

        .app-white img {
            width: 16vw;
        }

        .page-13-footer {
            bottom: 0;
            position: absolute;
            width: 100%;
            padding: 30px 50px;
            z-index: -1;
            background-color: var(--primary-color);
        }

        .social-white-txt img {
            display: block;
        }

        .social-white-txt {
            display: flex;
            align-items: center;
            gap: 6px;
            font-size: 14px;
            font-weight: 500;
        }

        .social-white-txt a {
            color: white;
        }

        .social-white {
            display: flex;
            align-items: center;
            justify-content: space-between;
        }

        .fill-icon {
            height: 28px;
            width: 28px;
            background-color: white;
            display: flex;
            justify-content: center;
            align-items: center;
            border-radius: 50%
        }

        .icon-image {
            width: 14px;
            height: 14px;
        }
    </style>
</head>

