    <script>
        const project_size = {{ project_size }}
        const total_cost = {{ total_cost }}
        const generation_calculation = [
            { month: "Jan", output: (project_size * 31 * 3.98).toFixed(2) },
            { month: "Fab", output: (project_size * 28 * 4.37).toFixed(2) },
            { month: "Mar", output: (project_size * 31 * 4.95).toFixed(2) },
            { month: "Apr", output: (project_size * 30 * 4.86).toFixed(2) },
            { month: "May", output: (project_size * 31 * 4.8).toFixed(2) },
            { month: "Jun", output: (project_size * 30 * 3.95).toFixed(2) },
            { month: "Jul", output: (project_size * 31 * 3.09).toFixed(2) },
            { month: "Aug", output: (project_size * 31 * 2.95).toFixed(2) },
            { month: "Sep", output: (project_size * 30 * 3.7).toFixed(2) },
            { month: "Oct", output: (project_size * 31 * 4.12).toFixed(2) },
            { month: "Nov", output: (project_size * 30 * 3.97).toFixed(2) },
            { month: "Dec", output: (project_size * 31 * 3.69).toFixed(2) },
        ]
        const ctx = document.getElementById('myChart');
        const cashFlow = Array.from({ length: 25 }).map((_, i) => i).reduce((acc, item) => {
            const preAccVal = acc[item - 1]
            if (item === 0) {
                const solar_unit_production = (project_size * 4 * 365).toFixed(2);
                const net_solar_saving = (solar_unit_production * 7.4).toFixed(2);
                const cash_flow = (net_solar_saving - total_cost).toFixed(2);
                acc.push({
                    solar_unit_production,
                    net_solar_saving,
                    cash_flow,
                })
            } else if (item === 1) {
                const solar_unit_production = (Number(preAccVal.solar_unit_production) - (Number(preAccVal.solar_unit_production) * 0.025)).toFixed(2);
                const net_solar_saving = (Number(solar_unit_production) * (Number(preAccVal.net_solar_saving) / Number(preAccVal.solar_unit_production))).toFixed(2);
                const cash_flow = (Number(net_solar_saving) + Number(preAccVal.cash_flow)).toFixed(2);
                acc.push({
                    solar_unit_production,
                    net_solar_saving,
                    cash_flow,
                })
            } else {
                const solar_unit_production = (Number(preAccVal.solar_unit_production) - (Number(acc[1].solar_unit_production) * 0.007)).toFixed(2);
                const net_solar_saving = (Number(solar_unit_production) * (Number(preAccVal.net_solar_saving) / Number(preAccVal.solar_unit_production))).toFixed(2)
                const cash_flow = (Number(net_solar_saving) + Number(preAccVal.cash_flow)).toFixed(2)
                acc.push({
                    solar_unit_production,
                    net_solar_saving,
                    cash_flow,
                })
            }
            return acc
        }, []).map(item => item.cash_flow)

        new Chart(ctx, {
            type: 'bar',
            data: {
                labels: Array.from({ length: 25 }).map((_, i) => i).map((item, index) => (new Date().getFullYear() + index)),
                datasets: [{
                    label: 'Saving',
                    data: cashFlow,
                    borderWidth: 1,
                    backgroundColor: '#0067CA',
                    borderColor: '#0067CA',
                }]
            },
            options: {
                devicePixelRatio: 2,
                scales: {
                    y: {
                        beginAtZero: false
                    }
                }
            }
        });

        const predictedEnergyCTX = document.getElementById('predictedEnergyOutput');

        new Chart(predictedEnergyCTX, {
            type: 'bar',
            data: {
                labels: generation_calculation?.map((calc) => calc.month),
                datasets: [{
                    label: 'Output (KW)',
                    data: generation_calculation?.map((calc) => Number(calc.output)),
                    borderWidth: 1,
                    backgroundColor: '#0067CA',
                    borderColor: '#0067CA',
                }]
            },
            options: {
                devicePixelRatio: 2,
                scales: {
                    y: {
                        beginAtZero: true
                    }
                }
            }
        })
    </script>
